generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id          String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title       String @db.VarChar
  description String @db.VarChar
  status      String @db.VarChar
}
